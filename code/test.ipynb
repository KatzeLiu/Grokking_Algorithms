    "%%nbtutor\n",
    "def max(list):\n",
    "    if len(list) == 2:\n",
    "        return list[0] if list[0] > list[1] else list[1]\n",
    "    sub_max = max(list[1:])\n",
    "    return list[0] if list[0] > sub_max else sub_max   \n",
    "\n",
    "arr = [1, 3, 7, 2]\n",
    "max(arr)"
   ]
